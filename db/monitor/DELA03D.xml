<?xml version="1.0"?>
<monitor name="Dell 1909W (DVI)" init="standard">
	<caps add="(vcp(04 05 08 10 12 14(05 08 0B 0C) 16 18 1A 52 60(01 03) 68(00 01 02 03 04 05 06) AC AE B2 B6 C6 C8 C9 D6(01 04 05) DC(00 02 03 04 05 06) DF FD))"/>
	<controls>
		<control id="colorpreset" address="0x14">
			<value id="normal"  value="0x05"/>
			<value id="cool"    value="0x08"/>
			<value id="warm"    value="0x0B"/>
			<value id="custom"  value="0x0C"/>
		</control>
		<control id="magicbright" address="0xdc">
			<value id="standard"   value="0"/>
			<value id="multimedia" value="2"/>
			<value id="movie"      value="3"/>
			<value id="nature"     value="4"/>
			<value id="game"       value="5"/>
			<value id="sports"     value="6"/>
		</control>
		<control id="language" address="0x68">
			<value id="english"  value="0"/>
			<value id="spanish"  value="1"/>
			<value id="french"   value="2"/>
			<value id="german"   value="3"/>
			<value id="russian"  value="4"/>
			<value id="chinese"  value="5"/>
			<value id="japanese" value="6"/>
		</control>
	</controls>
	<include file="VESA"/>
</monitor>
